{
    "name": "sense-components",
    "version": "0.1.3",
    "description": "Components to be used (in Widgets) in Qlik Sense.",
    "keywords": [
        "components",
        "widget"
    ],
    "homepage": "https://github.com/stefanwalther/sense-components",
    "bugs": {
        "url": "https://github.com/stefanwalther/sense-components/issues"
    },
    "license": "MIT",
    "author": "Stefan Walther (https://github.com/stefanwalther)",
    "repository": "stefanwalther/sense-components",
    "scripts": {
        "build": "sense-go build",
        "test": "sense-go build && karma start test/karma.conf.js",
        "preinstall": "npm list sense-go -g || npm install sense-go -g"
    },
    "dependencies": {},
    "devDependencies": {
        "angular": "^1.5.8",
        "angular-mocks": "^1.5.8",
        "chai": "^3.5.0",
        "flatpickr": "github:chmln/flatpickr#2.0",
        "gulp-format-md": "^0.1.10",
        "istanbul": "^0.4.4",
        "jquery": "^3.1.0",
        "karma": "^1.1.2",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.1.1",
        "karma-mocha": "^1.1.1",
        "karma-mocha-reporter": "^2.1.0",
        "karma-phantomjs-launcher": "^1.0.1",
        "karma-phantomjs2-launcher": "^0.5.0",
        "karma-requirejs": "^1.0.0",
        "loglevel": "^1.4.1",
        "mocha": "^3.0.0",
        "nouislider": "github:leongersen/noUiSlider",
        "require-css": "^0.1.8",
        "requirejs": "^2.2.0",
        "text": "github:requirejs/text",
        "underscore": "^1.7.0",
        "wnumb": "github:leongersen/wnumb"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "private": true,
    "verb": {
        "run": true,
        "toc": {
            "render": true,
            "method": "preWrite",
            "maxdepth": 2,
            "footer": false
        },
        "twitter": "waltherstefan",
        "layout": "empty",
        "tasks": [
            "readme"
        ],
        "plugins": [
            "gulp-format-md"
        ],
        "related": {
            "list": []
        },
        "reflinks": [
            "verb",
            "verb-generate-readme"
        ]
    }
}
